FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      changeDictionaryDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dictionaryReplacement
{
    boundary
    {
        parede
        {
	    type            symmetry;
            // type            fixedValue;
            // value           uniform 0;
        }
	
	extremos
        {
            type            fixedValue;
            value           uniform 0;
        }

    }

    T
    {
        internalField   uniform 300;

        boundaryField
        {
            ".*"
            {
                type            zeroGradient;
	    }
            
	    parede
	    {
		type            symmetry;
	    }
	    
	    cladding_to_coolant
            {
                type            compressible::turbulentTemperatureCoupledBaffleMixed;
                Tnbr            T;
                kappa           solidThermo;
                kappaName       none;
		value           uniform 300;
	    }

	    cladding_to_fuel
            {
                type            compressible::turbulentTemperatureCoupledBaffleMixed;
                Tnbr            T;
                kappa           solidThermo;
                kappaName       none;
		//thicknessLayers (0.001);
		//kappaLayers     (1);
                value           uniform 300;
            }
        }
    }
}

