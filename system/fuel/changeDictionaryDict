FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      changeDictionaryDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dictionaryReplacement
{
    boundary
    {
        parede
        {
	    type            symmetry;
            // type            fixedValue;
            // value           uniform 0;
        }

       extremos
       {
            type            fixedValue;
            value           uniform 0;
       }
    }

    T
    {
        internalField   uniform 300;
	// Temperatura inicial igual para todos

        boundaryField
        {
            ".*"
            {
	        type            zeroGradient; 
            }

	    parede
	    {
		type            symmetry;
	    }

	    //            "fuel_to_.*"
	    fuel_to_cladding
            {
                type            compressible::turbulentTemperatureCoupledBaffleMixed;
                Tnbr            T;
                kappa           solidThermo;
                kappaName       none;
		thicknessLayers (0.001);
		kappaLayers     (1);
                value           uniform 300;
            }
        }
    }

    Q
    {
        internalField   uniform 443.56; //900.0; //443.56; //4000; //38888888.9;
	// 250kW * combustível B1 (mais quente) 1.55 do total *1/4 do combustível
	// = 0.96875 kW -> 968.75 W
	// volume do fuel -> 5.5 [cm] * 5.5 [cm] * 72.2 [cm]
	// = 0.00218405 [m^3]
	// Q = 443.56

        boundaryField
        {
            ".*"
            {
	        type            calculated; 
            }

	    parede
	    {
		type            symmetry;
	    }

        }
    }


}

