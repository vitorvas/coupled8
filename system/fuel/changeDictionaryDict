FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      changeDictionaryDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dictionaryReplacement
{
    boundary
    {
        parede
        {
	    type            symmetry;
            // type            fixedValue;
            // value           uniform 0;
        }

       extremos
       {
            type            fixedValue;
            value           uniform 0;
       }
    }

    T
    {
        internalField   uniform 400;

        boundaryField
        {
            ".*"
            {
	        type            zeroGradient; 
            }

	    parede
	    {
		type            symmetry;
	    }

	    //            "fuel_to_.*"
	    fuel_to_cladding
            {
                type            compressible::turbulentTemperatureCoupledBaffleMixed;
                Tnbr            T;
                kappa           solidThermo;
                kappaName       none;
		thicknessLayers (0.01);
		kappaLayers     (1);
                value           uniform 400;
            }
        }
    }

    Q
    {
        internalField   uniform 80; //4000; //38888888.9;

        boundaryField
        {
            ".*"
            {
	        type            calculated; 
            }

	    parede
	    {
		type            symmetry;
	    }

        }
    }


}

